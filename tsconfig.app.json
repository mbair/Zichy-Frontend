/* To learn more about this file see: https://angular.io/config/tsconfig. */
{
  "extends": "./tsconfig.json",
	"compilerOptions": {
		"sourceMap": true,
		"declaration": false,
		"moduleResolution": "node",
		"emitDecoratorMetadata": true,
		"experimentalDecorators": true,
		"lib": [
			"es2016",
			"dom"
		],
		"outDir": "./out-tsc/app",
		"target": "es2015",
		"module": "es2020",
		"baseUrl": "",
		"types": []
	},
	"files": [
		"src/main.ts",
		"src/polyfills.ts"
	],
  "include": [
    "src/**/*.d.ts"
  ]
}
